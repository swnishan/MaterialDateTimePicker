<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="MaterialTimePickerView">
        <attr name="materialTimePickerTextAppearance" format="reference"/>
        <attr name="materialTimePickerHighlighterColor" format="color"/>
        <attr name="android:background"/>
        <attr name="materialTimePickerTimeConvention" format="enum">
            <enum name="HOURS_24" value="0"/>
            <enum name="HOURS_12" value="1"/>
        </attr>
    </declare-styleable>

    <declare-styleable name="MaterialTimePickerOverlay">
        <attr name="materialTimePickerViewStyle" format="reference"/>
    </declare-styleable>

</resources>